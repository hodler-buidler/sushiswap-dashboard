{
	"name": "sushi-dashboard",
	"license": "MIT",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"prepare": "husky install",
		"postinstall": "husky install",
		"build": "tsc && vite build",
		"dev": "vite --open",
		"serve": "vite preview",
		"serve:test": "start-server-and-test 'yarn serve' http://localhost:5000",
		"test": "yarn serve:test 'cypress open'",
		"test:headless": "yarn serve:test 'cypress run'",
		"lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|tsx)\"",
		"prettier": "prettier --ignore-path .gitignore \"src/**/*.+(ts|tsx)\"",
		"format": "prettier --write --ignore-path .gitignore \"src/**/*.+(ts|tsx)\"",
		"check-format": "prettier --list-different --ignore-path .gitignore \"src/**/*.+(ts|tsx)\"",
		"types-check": "tsc --noEmit",
		"lint-staged": "lint-staged"
	},
	"dependencies": {
		"@apollo/client": "^3.7.0",
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"apexcharts": "^3.35.5",
		"color": "^4.2.3",
		"dayjs": "^1.11.5",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"i18next": "^21.9.2",
		"normalize.css": "^8.0.1",
		"react": "^18.2.0",
		"react-apexcharts": "^1.4.0",
		"react-dom": "^18.2.0",
		"react-helmet": "^6.1.0",
		"react-i18next": "^11.18.6",
		"react-loading-skeleton": "^3.1.0",
		"react-router-dom": "^6.4.1"
	},
	"devDependencies": {
		"@cypress/code-coverage": "3.9.11",
		"@nabla/vite-plugin-eslint": "1.3.2",
		"@originjs/vite-plugin-require-context": "^1.0.9",
		"@types/color": "^3.0.3",
		"@types/react": "^18.0.21",
		"@types/react-dom": "^18.0.6",
		"@types/react-helmet": "^6.1.5",
		"@typescript-eslint/eslint-plugin": "4.33.0",
		"@typescript-eslint/parser": "4.33.0",
		"@vitejs/plugin-react": "1.0.7",
		"autoprefixer": "10.4.0",
		"cypress": "8.7.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-import": "2.25.2",
		"eslint-plugin-jsx-a11y": "6.4.1",
		"eslint-plugin-react": "7.26.1",
		"eslint-plugin-react-hooks": "4.2.0",
		"eslint-plugin-react-prefer-function-component": "0.0.7",
		"husky": "7.0.4",
		"lint-staged": "11.2.6",
		"npm-run-all": "4.1.5",
		"pinst": "^2.1.6",
		"prettier": "2.4.1",
		"rollup": "2.58.3",
		"rollup-plugin-istanbul": "3.0.0",
		"sass": "^1.55.0",
		"start-server-and-test": "1.14.0",
		"typescript": "4.4.4",
		"vite": "2.6.13",
		"vite-plugin-require": "^1.0.5",
		"vite-tsconfig-paths": "3.3.17",
		"workbox-build": "6.3.0",
		"workbox-window": "6.3.0"
	},
	"browserslist": {
		"production": "Edge >= 18, Firefox >= 60, Chrome >= 61, Safari >= 11, Opera >= 48",
		"development": [
			"last 1 chrome version",
			"last 1 firefox version"
		]
	}
}
